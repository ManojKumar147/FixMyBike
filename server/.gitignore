# .gitignore (inside server/)

# Ignore node_modules folder
node_modules/

# Ignore build output (if applicable)
/build/
/dist/

# Ignore environment variable files
.env

# Ignore logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# IDE/editor specific files
.vscode/
.idea/

# Ignore any database backups
*.db
*.sqlite

# Ignore test coverage reports (if any)
/coverage/
# Ignore all files in the config folder
server/src/config/
